{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    // "airbnb/base"
    "eslint:recommended",
    "plugin:react/recommended",
    "prettier"
  ],
  // "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "react/prop-types": 0
  }
}
